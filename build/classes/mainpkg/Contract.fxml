<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="656.0" prefWidth="1113.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.ContractController">
   <children>
      <ImageView fitHeight="105.0" fitWidth="92.0">
         <image>
            <Image url="@../../../../BFF.png" />
         </image>
      </ImageView>
      <Label layoutX="102.0" layoutY="18.0" prefHeight="69.0" prefWidth="402.0" text="Bangladesh Football Fedaration" wrapText="true">
         <font>
            <Font name="Arial Bold" size="25.0" />
         </font>
      </Label>
      <Line endX="959.0" endY="-24.0" layoutX="155.0" layoutY="138.0" startX="-154.0" startY="-24.0" strokeWidth="4.0" />
      <Text layoutX="14.0" layoutY="153.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Contract Table">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Text>
      <TableView layoutX="14.0" layoutY="169.0" prefHeight="200.0" prefWidth="1091.0">
        <columns>
          <TableColumn fx:id="playerContractTableColumn" prefWidth="353.0" text="Player Contract" />
          <TableColumn fx:id="clubContractTableColumn" prefWidth="383.0" text="Club Contract" />
            <TableColumn fx:id="statusContractTableColumn" prefWidth="350.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="404.0" mnemonicParsing="false" onAction="#approveContractButtonOnClicked" text="Approve">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="517.0" layoutY="404.0" mnemonicParsing="false" onAction="#rejectContractButtonOnClicked" text="Reject">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label fx:id="statusContractLabel" layoutX="14.0" layoutY="482.0" prefHeight="147.0" prefWidth="1091.0" style="-fx-background-color: #a7ffeb;" />
      <Button layoutX="1033.0" layoutY="404.0" mnemonicParsing="false" onAction="#backButtonOnClicked" text="Back">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
